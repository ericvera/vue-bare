{
  "name": "vue-bare",
  "version": "0.6.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "!/**/__test__",
    "!*.test.*"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "smoke": "yarn build && yarn lint",
    "-- PRE-COMMIT HOOKS --": "",
    "localAfterInstall": "husky || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "packageManager": "yarn@4.6.0",
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.20.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "pinst": "^3.0.0",
    "prettier": "^3.5.0",
    "typedoc": "^0.27.7",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericvera/vue-bare"
  },
  "keywords": [
    "vue",
    "bare",
    "unstyled",
    "components"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,mjs}": "eslint --cache",
    "*": "prettier --ignore-unknown --write"
  }
}
